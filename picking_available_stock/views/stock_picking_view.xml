<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!-- Inherit Form View to Modify it -->
    <record id="stock_picking_available_inherit" model="ir.ui.view">
        <field name="name">stock_picking_available_inherit</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">

            <xpath expr="//field[@name='move_ids_without_package']/tree/field[@name='product_qty']" position="after">
                <!-- Add your fields or attributes here -->
                <field name="qty_available_location" string="Cantidad disponible" readonly="1"/>
            </xpath>

        </field>
    </record>


</odoo>
