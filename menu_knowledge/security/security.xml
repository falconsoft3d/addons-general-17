<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="knowledge_category" model="ir.module.category">
            <field name="name">Knowledge</field>
            <field name="description">Knowledge</field>
            <field name="sequence" eval="90"/>
        </record>

        <record id="group_user_knowledge" model="res.groups">
            <field name="name">User knowledge</field>
            <field name="category_id" ref="knowledge_category"/>
            <field name="implied_ids" eval="[(4, ref('base.user_root'))]"/>
        </record>
</odoo>
