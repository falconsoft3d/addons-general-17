<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="import_view_move_tree" model="ir.ui.view">
            <field name="name">import.view.move.tree</field>
            <field name="model">account.move</field>
            <field name="inherit_id" ref="account.view_move_tree"/>
            <field name="arch" type="xml">
                <xpath expr="//field[@name='state']" position="after">
                    <field name="import_account_move_id" optional="hide"/>
                    <field name="old_id" optional="hide"/>
                </xpath>
            </field>
        </record>
    </data>
</odoo>