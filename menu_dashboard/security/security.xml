<?xml version="1.0" encoding="utf-8"?>
<odoo>
        <record id="dashboard_category" model="ir.module.category">
            <field name="name">Dashboard</field>
            <field name="description">Dashboard</field>
            <field name="sequence" eval="90"/>
        </record>

        <record id="group_user_dashboard" model="res.groups">
            <field name="name">User Dashboard</field>
            <field name="category_id" ref="dashboard_category"/>
            <field name="implied_ids" eval="[(4, ref('base.user_root'))]"/>
        </record>
</odoo>
